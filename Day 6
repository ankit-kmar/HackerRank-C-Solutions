#include <iostream>

using namespace std;

int main(){
    int s;
    cin>>s;

    for(int i = 0;i<s;i++)
{
    string str;
    cin>>str;

    for(int j = 0;j<str.length();j++)
    {
        if(j%2==0)
        cout<<str[j];
    }
    cout<<" ";
      for(int j = 0;j<str.length();j++)
    {
        if(j%2!=0)
        cout<<str[j];
    }
cout<<endl;

}
    
    
    return 0;

}



